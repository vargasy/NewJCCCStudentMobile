        </activity>
        <activity
            android:name=".CampusActivity"
            android:label="@string/title_activity_campus_life"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".WellnessActivity"
            android:label="@string/title_activity_well"
