        <activity
            android:name=".DiscountDetailsActivity"
            android:label="@string/title_activity_discounts" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WellnessDetailsActivity"
            android:label="@string/title_activity_well" >
